                        "issue": {
                            "type": "object"
                        },
                        "changes": {
                            "type": "object"
                        },
                        "repository": {
                            "type": "object"
                        },
                        "sender": {
                            "type": "object"
                        }
                    }
                }
            }
        }
    }
}